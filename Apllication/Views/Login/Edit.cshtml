@using BOL
@{
    ViewData["Title"] = "Login Page";
    Atharva O=this.ViewData["id"] as Atharva;
}

<div class="text-center">
<h2>Edit Profile</h2><br>
<form method="post">
 Enter ID:   <input type="text" name="id" value="@O"><br><br>
 Enter New First name: <input type="text" name="fname"><br><br>
 Enter New last name: <input type="text" name="lname"><br><br>
 <button>Submit</button>
 </form>

</div>
